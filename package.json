{
  "name": "john-stack",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "client:dev": "NODE_OPTIONS='--import tsx/esm' vite ./src/client --port=5002 --strictPort",
    "client:build": "NODE_OPTIONS='--import tsx/esm' vite build ./src/client --outDir=../../dist/client --emptyOutDir",
    "client:preview": "NODE_OPTIONS='--import tsx/esm' vite preview ./src/client --outDir=../../dist/client --port=5000 --strictPort",
    "server:dev": "tsx --env-file=.env --watch ./src/server/main.ts",
    "server:build": "esbuild ./src/server/main.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outdir=dist/server"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.658.1",
    "@effect/schema": "^0.67.21",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/joy": "^5.0.0-beta.48",
    "@tanstack/react-query": "^5.40.1",
    "@zoom/videosdk": "^1.12.5",
    "async-mutex": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "effect": "^3.3.0",
    "jsrsasign": "^11.1.0",
    "pinata": "^1.4.1",
    "react": "^18.2.0",
    "react-dice-complete": "^2.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "ts-pattern": "^5.4.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsrsasign": "^10.5.14",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "body-parser": "^1.20.2",
    "esbuild": "^0.23.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.19.2",
    "prettier": "3.3.1",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.0.8"
  }
}
